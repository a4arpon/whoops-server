{
  "version": 2,
  "builds": [
    {
      "src": "./dist/index.ts",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "yarn build && yarn start",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
